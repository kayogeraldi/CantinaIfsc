<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="pu_Cantina" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>model.bo.Bairro</class>
    <class>model.bo.Cidade</class>
    <class>model.bo.Endereco</class>
    <class>model.bo.Caixa</class>
    <class>model.bo.Carteirinha</class>
    <class>model.bo.Cliente</class>
    <class>model.bo.Compra</class>
    <class>model.bo.Contas</class>
    <class>model.bo.Fornecedor</class>
    <class>model.bo.Funcionario</class>
    <class>model.bo.ItemCompra</class>
    <class>model.bo.ItemVenda</class>
    <class>model.bo.MovimentacaoEstoque</class>
    <class>model.bo.MovimentoCaixa</class>
    <class>model.bo.Produto</class>
    <class>model.bo.Venda</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/cantinajk?verifyServerCertificate=false&amp;useSSL=false&amp;requireSSL=false&amp;serverTimezone=UTC"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="ifsc"/>
    </properties>
  </persistence-unit>
</persistence>
